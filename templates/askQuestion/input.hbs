<div id= 'writeContent'>
  <textarea class = 'text'>{{getString type}}</textarea>
  <div class="action" data-component="{{component}}" data-component-id="{{componentId}}" data-parent="{{parent}}">
    <span class="save" data-event="save">save</span>
    <span class="cancel" data-event="cancel">cancel</span>
  </div>
</div>
